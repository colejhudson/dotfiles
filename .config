#!/usr/bin/env fish

if test -d $HOME/.opam
  bass source $HOME/.opam/opam-init/init.sh > /dev/null 2> /dev/null; or true
end

if test -d $NVM_DIR
  if test -f $NVM_DIR/nvm.sh
    bass source $NVM_DIR/nvm.sh
  end

  if test -f $NVM_DIR/bash_completion
    bass source $NVM_DIR/bash_completion
  end
end

if test -f /Users/work/.nix-profile/etc/profile.d/nix.sh
  bass source /Users/work/.nix-profile/etc/profile.d/nix.sh
end
