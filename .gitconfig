[user]
  email = cole@colejhudson.com
  name = Cole Hudson
  username = colejhudson
  signingkey = "49667D13599D4AF837EA646E2F7DB0FBBB9FEFC7" 
[core]
  editor = vim
  whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
  excludesfile = ~/.gitignore
[web]
  browser = firefox
[credential]
  helper = cache --timeout 7200
[transfer]
  fsckObjects = false
[fetch]
  fsckobjects = false
[receive "fsck"]
  zeroPaddedFilemode = warn
[github]
  user = colejhudson
[alias]
  s = status
  c = commit
  b = branch
  d = diff
  ap = add --patch
  ra = remote add
  rr = remote rename
  hub = !hub
  lab = !lab
  commit = commit -S
  push-all = !git remote | xargs -L1 git push
  hello-world = commit -S -m 'initial commit'
  delete-whitespace = !sed --in-place 's/[[:space:]]\\+$//'
[commit]
  gpgsign = true
